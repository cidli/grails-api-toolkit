/* JSON API Object */

{
    "test": {
        "VALUES": {
            "id": {                
                "type": "PKEY"
            },
            "testdata": {
                "type": "String",
                "mockData": "blahblahblah"
            }
        },
        "URI": {
            "test/fred": {
            	"METHOD":"GET",
            	"DESCRIPTION":"Get test data",
            	"ROLES":["ROLE_ADMIN","ROLE_USER"],
                "REQUEST": {
                    "permitAll":["id"]
                },
                "RESPONSE": {
                	"permitAll":["id"],
                    "ROLE_ADMIN":["testdata"],
                    "ROLE_USER":["testdata"]
                }
            },
            "test/show": {
            	"METHOD":"GET",
            	"DESCRIPTION":"Get test data",
            	"ROLES":["ROLE_ADMIN","ROLE_USER"],
                "REQUEST": {
                    "permitAll":["id"]
                },
                "RESPONSE": {
                	"permitAll":["id"],
                    "ROLE_ADMIN":["testdata"],
                    "ROLE_USER":["testdata"]
                }
            },
            "test/create": {
            	"METHOD":"PUT",
            	"DESCRIPTION":"Create test data",
            	"ROLES":["ROLE_ADMIN","ROLE_USER"],
                "REQUEST": {
                    "permitAll":["testdata"]
                },
                "RESPONSE": {
                    "permitAll":["id"]
                },
                "ERROR":{"304_NOT_MODIFIED":""}
            },
             "test/update": {
            	"METHOD":"POST",
            	"DESCRIPTION":"Update test data",
            	"ROLES":["ROLE_ADMIN","ROLE_USER"],
                "REQUEST": {
                    "permitAll":["id","testdata"]
                },
                "RESPONSE": {
                    "permitAll":["id"]
                },
                "ERROR":{"304_NOT_MODIFIED":""}
            }
        }
    }
